{
  "dependencies": {
    "concurrently": "^8.2.0",
    "if-env": "^1.0.4"
  },
  "name": "shoppinglist",
  "version": "1.0.0",
  "scripts": {
    "build": "if-env TYPE=server && (cd server && npm install) || (cd client && npm install)",
    "start": "node ./server/server.js",
    "client": "cd client && npm run start",
    "dev": "concurrently \"npm start\" \"npm run client\""
  },
  "author": "Veikkap",
  "license": "ISC"
}
